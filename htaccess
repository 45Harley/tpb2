
DirectoryIndex index.php index.html

# Clean state/town URLs (case-insensitive)
RewriteEngine On

# Exclude qt from state rewrite
RewriteRule ^qt/ - [L]

RewriteCond %{REQUEST_URI} ^/([a-zA-Z]{2})/(.*)$
RewriteRule ^([a-zA-Z]{2})/(.*)$ z-states/$1/$2 [L,NC]

